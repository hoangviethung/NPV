.container
	.row
		.col-lg-8.form-dangky-booking
			.title.wow.fadeIn(data-wow-delay=".3s")
				h3 ĐĂNG KÍ BOOKING
			form(action="#" method="method")
				.form-row
					.form-group.col-lg-6
						label.color-red(for="Id_partner") PartnerID
						input#Id_partner(type="text" placeholder="ID Booking Note")
					.form-group.col-lg-6
						label(for="email") Email
						input#email(type="text" placeholder="Email")
					.form-group.col-lg-6
						label(for="spvmail") Person in charges of NPV (Mail)
						input#spvmail(type="text" placeholder="Person in charges of ASL (Mail)")
					.form-group.col-lg-6
						label(for="") Cargo ready date
						input(type="date" placeholder="Ngày hàng sẵn sàng* (dd/mm/yyyy)")
					.form-group.col-lg-6
						label(for="shipper") Shipper
						textarea#shipper(name="" placeholder="Shipper").small
					.form-group.col-lg-6
						label(for="consignee") Consignee
						textarea#consignee(name="" placeholder="Consignee").small
					.form-group.col-lg-6
						label(for="portofdischarge") Port of discharge
						input#portofdischarge(type="text" placeholder="Port of discharge")
					.form-group.col-lg-6
						label(for="portofloading") Port of loading
						input#portofloading(type="text" placeholder="Port of loading")
					.form-group.col-lg-6
						label(for="placeofdelivery") Place of delivery
						input#placeofdelivery(type="text" placeholder="Place of delivery")
					.form-group.col-lg-6
						label(for="grossweight") Commodity
						input#commodity(type="text" placeholder="Commodity")
					.form-group.col-lg-6
						label(for="") Freight: Prepaid or Collect*
						select(name="")
							option(value="") Prepaid
							option(value="") TEST
							option(value="") TEST
							option(value="") TEST
							option(value="") TEST
							option(value="") TEST
					.form-group.col-lg-6
						label Type of move
						select#type_Of_Move(name="")
							option(value='FCL') FCL
							option(value='LCL') LCL
							option(value='AIR') AIR
					.form-group.col-lg-6.FCL
						label(for="Contdropoffplace") Cont drop off place
						input#Contdropoffplace(type="text" placeholder="Cont drop off place*")
					.form-group.col-lg-3.FCL
						label(for="Quantity") Quantity
						input#Quantity(type="number" placeholder="Quantity")
					.form-group.col-lg-3.FCL
						label Type of move*
						select(name="")
							option(value='FL20') 20 FL
							option(value='GOH20') 20 GOH
							option(value='OT20') 20 OT
							option(value='RF20') 20 RF
							option(value='DC40') 40 DC
							option(value='FL40') 40 FL
							option(value='HC40') 40 HC
							option(value='HR40') 40 HR
							option(value='OT40') 40 OT
							option(value='RF40') 40 RF
							option(value='GOH40') 40 GOH
							option(value='HC45') 45 HC
					.form-group.col-lg-12.LCL.hidden
						label(for="Measurement(CBM)") Measurement (CBM)
						input#Measurement(type="text" placeholder="Measurement (CBM)")
					.form-group.col-lg-6.AIR.hidden
						label(for="GrossWeight") Gross Weight
						input#GrossWeight(type="text" placeholder="Gross Weight")
					.form-group.col-lg-6.AIR.hidden
						label(for="Volume") Volume
						input#Volume(type="text" placeholder="Volume")
					.form-group.col-lg-12
						p Details of goods: Quantity/G.W/Goods name/PO & style number/Measurement
						label#buttonchoose(for="file") Choose File
						input#file(type="file" hidden)
					.form-group.col-lg-12
						label(for="remark") Remark
						textarea#remark(name="" placeholder="Remark").big
				button.btn.bg-blue ĐĂNG KÍ
		.col-lg-4
			include _aside-booking.pug