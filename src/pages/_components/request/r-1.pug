.container
	.row
		.col-lg-8.request
			.title.wow.fadeIn(data-wow-delay=".3s")
				h3 ĐĂNG KÝ SHIPPING INSTRUCTION
			form(action="#" method="method")
				.form-row
					.form-group.col-lg-6
						label.color-red(for="Id_booking") ID Booking Note*
						input#Id_booking(type="text" placeholder="ID Booking Note")
					.form-group.col-lg-6
						label(for="Id_partner") ID Booking Note*
						input#Id_partner(type="text" placeholder="Partner ID (nếu có)")
					.form-group.col-lg-6
						label(for="shipper") Shipper*
						textarea#shipper(name="" placeholder="Shipper").small
					.form-group.col-lg-6
						label(for="consignee") Consignee*
						textarea#consignee(name="" placeholder="Consignee").small
					.form-group.col-lg-6
						label(for="notifyparty") Notify Party*
						textarea#notifyparty(name="" placeholder="Notify Party*").small
					.form-group.col-lg-6
						label(for="descriptionofgoods") Description of goods*
						textarea#descriptionofgoods(name="" placeholder="Description of goods*").small
					.form-group.col-lg-6
						label(for="portofloading") Port of loading*
						input#portofloading(type="text" placeholder="Port of loading")
					.form-group.col-lg-6
						label(for="portofdischarge") Port of discharge*
						input#portofdischarge(type="text" placeholder="Port of discharge*")
					.form-group.col-lg-6
						label(for="noofpackages") No of packages*
						input#noofpackages(type="text" placeholder="No of packages*")
					.form-group.col-lg-6
						label(for="grossweight") Gross Weight*
						input#grossweight(type="text" placeholder="Gross Weight*")
					.form-group.col-lg-6
						label(for="measurement") Measurement*
						input#measurement(type="text" placeholder="Measurement*")
					.form-group.col-lg-6
						label(for="shippingmarks") Shipping marks*
						textarea#shippingmarks(name="" placeholder="Shipping marks*").small
					.form-group.col-lg-6
						label(for="") Frieght*
						select(name="")
							option(value="") Prepaid
							option(value="") TEST
							option(value="") TEST
							option(value="") TEST
							option(value="") TEST
							option(value="") TEST
					.form-group.col-lg-6
						label(for="") Type of BL*
